{
  "name": "salon-tablet",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "mdc": "npm run mdc:linter & npm run mdc:link-check",
    "mdc:linter": "markdownlint --ignore node_modules/ .",
    "mdc:markdown-link-check": "markdown-link-check",
    "mdc:link-check": "find . -type d -name node_modules -prune -o -type f -name *.md -exec npm run mdc:markdown-link-check {} \\;",
    "mdc:l": "markdown-link-check --help ",
    "tsc": "tsc",
    "test": "jest",
    "tests": "jest src/.*/*.test.tsx?$",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:show": "eslint-nibble ./src --ext .js,.jsx,.ts,.tsx",
    "lint:showerror": "eslint-nibble ./src --ext .js,.jsx,.ts,.tsx --no-warnings",
    "format": "prettier --check 'src'",
    "format:fix": "prettier --write 'src'",
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 next -p 8080",
    "https": "HTTPS=https://localhost:8080 NODE_OPTIONS=--max_old_space_size=4096 ts-node --project tsconfig.server.json src/server/serverHttps.ts",
    "storybook": "start-storybook -p 6006",
    "build:test": "rm -rf ./build && NODE_ENV=test next build",
    "build:production": "rm -rf ./build && NODE_ENV=production next build",
    "start": "next start -p 8080",
    "deploy:test": "npm run build:test && gcloud app deploy appTest.yaml --quiet",
    "deploy:production": "npm run build:production && gcloud app deploy app.yaml --quiet"
  },
  "author": "kazuhideoki",
  "license": "ISC",
